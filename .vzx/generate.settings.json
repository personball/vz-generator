[
  {
    "option": "Create Pinia",
    "templateSyntax": "Liquid",
    "templatePath": "./.vzx/templates/samples/vue.pinia/{{store}}.js",
    "variables": [
      {
        "name": "store",
        "type": "String"
      },
      {
        "name": "model",
        "type": "String"
      }
    ],
    "output": "./src/pinia/modules/{{store|camelCase}}.js"
  },
  {
    "option": "Create Abp",
    "templateSyntax": "Liquid",
    "templatePath": "./.vzx/templates/samples/abp",
    "variables": [
      {
        "name": "project",
        "type": "String"
      },
      {
        "name": "entity",
        "type": "String"
      }
    ],
    "output": "./output/"
  },
  {
    "option": "Swagger2Api",
    "templateSyntax": "Liquid",
    "templatePath": "./.vzx/templates/samples/swagger2api",
    "variables": [
      {
        "name": "name",
        "type": "String"
      },
      {
        "name": "module",
        "type": "String"
      }
    ],
    "output": "./src/apis/modules/{{name}}.js"
  },
  {
    "option": "Swagger2View",
    "templateSyntax": "Liquid",
    "templatePath": "./.vzx/templates/samples/swagger2view",
    "variables": [
      {
        "name": "name",
        "type": "String"
      },
      {
        "name": "module",
        "type": "String"
      },
      {
        "name": "swagger",
        "type": "JsonFile",
        "defaultValue": "./.vzx/templates/samples/swagger2view/swagger.json"
      }
    ],
    "output": "./src/view/{{module}}/{{name}}.vue"
  }
]